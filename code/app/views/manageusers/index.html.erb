<h1>All users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
	<th>Address</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.address %></td>
    </tr>
  <% end %>
</table>

<%= link_to "Create New User", action: "new" %>